#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules/@yao-pkg/pkg/lib-es5/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules/@yao-pkg/pkg/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules/@yao-pkg/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules/@yao-pkg/pkg/lib-es5/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules/@yao-pkg/pkg/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules/@yao-pkg/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/@yao-pkg+pkg@6.5.1_encoding@0.1.13/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@yao-pkg/pkg/lib-es5/bin.js" "$@"
else
  exec node  "$basedir/../@yao-pkg/pkg/lib-es5/bin.js" "$@"
fi
