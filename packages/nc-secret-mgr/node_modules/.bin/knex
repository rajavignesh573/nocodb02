#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/knex@3.1.0_mysql@2.18.1_pg@8.16.0_sqlite3@5.1.6_encoding@0.1.13_/node_modules/knex/bin/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/knex@3.1.0_mysql@2.18.1_pg@8.16.0_sqlite3@5.1.6_encoding@0.1.13_/node_modules/knex/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/knex@3.1.0_mysql@2.18.1_pg@8.16.0_sqlite3@5.1.6_encoding@0.1.13_/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/knex@3.1.0_mysql@2.18.1_pg@8.16.0_sqlite3@5.1.6_encoding@0.1.13_/node_modules/knex/bin/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/knex@3.1.0_mysql@2.18.1_pg@8.16.0_sqlite3@5.1.6_encoding@0.1.13_/node_modules/knex/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/knex@3.1.0_mysql@2.18.1_pg@8.16.0_sqlite3@5.1.6_encoding@0.1.13_/node_modules:/home/nocodb/nocorun_vg/nocodb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../knex/bin/cli.js" "$@"
else
  exec node  "$basedir/../knex/bin/cli.js" "$@"
fi
